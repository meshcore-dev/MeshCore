<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data like encryption keys -->
        <exclude domain="sharedpref" path="." />
        <exclude domain="database" path="." />
    </cloud-backup>
    <device-transfer>
        <!-- Allow transfer of basic preferences only -->
        <include domain="file" path="." />
    </device-transfer>
</data-extraction-rules>
